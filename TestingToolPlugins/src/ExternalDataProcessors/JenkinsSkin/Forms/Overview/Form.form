    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	Планировщик.Задание КАК Задание,
	Планировщик.ВремяНачала КАК ВремяНачала,
	Планировщик.ВремяОкончания КАК ВремяОкончания,
	Планировщик.Интервал КАК Интервал,
	Планировщик.Включено КАК Включено,
	Планировщик.ДатаЗавершения КАК ДатаЗавершения,
	Планировщик.ДатаЗапуска КАК ДатаЗапуска,
	Планировщик.ИнтервалПовтораПриОшибке КАК ИнтервалПовтораПриОшибке,
	Планировщик.КоличествоПовторовПриОшибке КАК КоличествоПовторовПриОшибке,
	Планировщик.ПродолжатьПриОшибке КАК ПродолжатьПриОшибке,
	Планировщик.Понедельник КАК Понедельник,
	Планировщик.Вторник КАК Вторник,
	Планировщик.Среда КАК Среда,
	Планировщик.Четверг КАК Четверг,
	Планировщик.Пятница КАК Пятница,
	Планировщик.Суббота КАК Суббота,
	Планировщик.Воскресенье КАК Воскресенье,
	Планировщик.ИнтервалСек КАК ИнтервалСек,
	Планировщик.ВремяНачалаСек КАК ВремяНачалаСек,
	Планировщик.ВремяОкончанияСек КАК ВремяОкончанияСек,
	Планировщик.ДатаЗапускаСек КАК ДатаЗапускаСек,
	Планировщик.ДатаЗавершенияСек КАК ДатаЗавершенияСек,
	Планировщик.Однократно,
	Планировщик.Комментарий КАК Комментарий,
	ВЫБОР
		КОГДА ЕСТЬNULL(Запрос1.Задание, НЕОПРЕДЕЛЕНО) = НЕОПРЕДЕЛЕНО
			ТОГДА -1
		ИНАЧЕ 2
	КОНЕЦ КАК ВРаботе,
	ЕСТЬNULL(Запрос1.КоличествоЭтапов, 0) КАК КоличествоЭтапов,
	ЕСТЬNULL(Запрос1.КоличествоВыполненныхЭтапов, 0) КАК КоличествоВыполненныхЭтапов,
	ВЫБОР
		КОГДА ЕСТЬNULL(Запрос1.КоличествоЭтапов, 0) = 0
			ТОГДА 0
		ИНАЧЕ ВЫРАЗИТЬ(ЕСТЬNULL(Запрос1.КоличествоВыполненныхЭтапов, 0) / ЕСТЬNULL(Запрос1.КоличествоЭтапов, 0) * 100 КАК ЧИСЛО(10, 0))
	КОНЕЦ КАК ПроцентВыполненияЭтапов
ИЗ
	РегистрСведений.Планировщик КАК Планировщик
